{
  "name": "nodedocker",
  "version": "1.1.0",
  "repository": "https://github.com/grebnesorbocaj/nodeDocker",
  "description": "sample node api, musings about efficient apis and more",
  "main": "index.js",
  "scripts": {
    "docker:run": "echo \"run on localhost:8888\" && docker run -p 8888:3000 -it --rm diakou/test",
    "docker:build": "docker image rm diakou/test && docker build -t diakou/test .",
    "lint": "node_modules/.bin/eslint .",
    "lint:fix": "node_modules/.bin/eslint . --fix",
    "start": "node ./src/index.js",
    "start:dev": "nodemon ./src/index.js",
    "test": "jest --forceExit --collectCoverage"
  },
  "author": "diakou/grebnesorbocaj",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "verbose": true,
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  }
}
